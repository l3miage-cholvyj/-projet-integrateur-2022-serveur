drop table LesChamis;

create table LesChamis 
(
	login varchar(20) not null,
	ageChamis int,
	constraint LesChamis_pk primary key (login)
);

insert into LesChamis values ('carobis', 21);
insert into LesChamis values ('escribis', 43);
insert into LesChamis values ('momo', 38);
insert into LesChamis values ('nomoldu', 20);
insert into LesChamis values ('python38', 18);
insert into LesChamis values ('yes4moldus', 38);

